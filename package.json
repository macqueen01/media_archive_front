{
  "name": "svelte",
  "version": "1.0.0",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "sirv-cli": "^0.3.1",
    "tinro": "^0.6.12"
  },
  "dependencies": {
    "@tiptap/core": "^2.0.0-beta.199",
    "@tiptap/extension-paragraph": "^2.0.0-beta.199",
    "@tiptap/starter-kit": "^2.0.0-beta.199",
    "axios": "^0.27.2",
    "rollup-plugin-json": "^4.0.0",
    "svelte": "^3.32.3",
    "svelte-loading-spinners": "^0.3.4",
    "youtube-dl": "^3.5.0"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single --port 8080 --host 0.0.0.0",
    "start:dev": "sirv public --dev --port 8080",
    "routing-setup": "npm i -D tinro",
    "axios-setup": "npm i axios"
  },
  "keywords": [
    "svelte",
    "starter"
  ],
  "description": "Svelte example starter project"
}
